{
    "name": "samwilson/twyne",
    "description": "A blogging application.",
    "license": "GPL-3.0-or-later",
    "require": {
        "ext-pdo": "*",
        "cebe/markdown-latex": "^1.1",
        "krinkle/intuition": "^2.0",
        "league/flysystem": "^1.0",
        "nikic/fast-route": "^1.3",
        "phpmailer/phpmailer": "^6.1",
        "simplepie/simplepie": "^1.5",
        "symfony/console": "^4.1",
        "twbs/bootstrap": "^4.1",
        "twig/twig": "^2.4",
        "wikimedia/less.php": "^1.8"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.5",
        "mediawiki/mediawiki-codesniffer": "^28.0",
        "pds/skeleton": "^1.0",
        "phpunit/phpunit": "^8.4",
        "squizlabs/php_codesniffer": "^3.3",
        "symfony/var-dumper": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Samwilson\\Twyne\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Samwilson\\Twyne\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@post-install-update"
        ],
        "post-update-cmd": [
            "@post-install-update"
        ],
        "post-install-update": {
            "copy config": "php -r \"file_exists('config.php') || copy('config.example.php', 'config.php');\""
        },
        "test": [
            "composer validate",
            "composer normalize --dry-run",
            "vendor/bin/phpcs .",
            "vendor/bin/phpunit tests"
        ]
    }
}
